<div class="single-image-{{id}}">
  {% if section.settings.button_link != blank %}
  <a href="{{section.settings.button_link}}" {% if section.settings.open-new-tab != false %}target="_blank"{% endif %}> 
    {% endif %}
    <picture class="single-image-container">
      {% if image_mobile != blank %}
      <source 
              class="single-image-img single-image--lazy lazyload"
              media="(max-width: 500px)"
              data-srcset="{{ image_mobile | img_url:'600x' }}">
      <source 
              class="single-image-img single-image--lazy lazyload"
              media="(max-width: 767px)"
              data-srcset="{{ image_mobile | img_url:'867x' }}">
      {% endif %}
      <source 
              class="single-image-img single-image--lazy lazyload"
              media="(max-width: 1200px)"
              data-srcset="{{ image_desktop | img_url:'1200x' }}">
      <img 
           class="single-image-img single-image--lazy lazyload"
           data-src="{{ image_desktop | img_url: '1900x' }}" 
           alt="{{image_alt}}">
    </picture>

    {% if section.settings.button_link != blank %}
  </a>
  {% endif %}
  {% if section.settings.hide-arrow == false %}
  <div class="hero__arrow animation--bounce">
    <div class="arrow__container">
      <a href="#" id="arrowScroll-{{section.id}}"><svg xmlns="http://www.w3.org/2000/svg" id="Layer" enable-background="new 0 0 64 64" height="512" viewBox="0 0 64 64" width="512"><path d="m32 8c-1.104 0-2 .896-2 2v39.899l-14.552-15.278c-.761-.799-2.026-.832-2.828-.069-.8.762-.831 2.027-.069 2.827l16.62 17.449c.756.756 1.76 1.172 2.829 1.172 1.068 0 2.073-.416 2.862-1.207l16.586-17.414c.762-.8.73-2.065-.069-2.827-.799-.763-2.065-.731-2.827.069l-14.552 15.342v-39.963c0-1.104-.896-2-2-2z"/></svg></a>

    </div>
  </div>
  {% endif %}
</div>

<style>
  .single-image-{{id}}{
    text-align: center; 
    margin-top: {{section.settings.top-spacing}}px;
    margin-bottom: {{section.settings.bottom-spacing}}px;
    position: relative;
  }
  .single-image-{{id}} img{
    width: 100%;
  }


  {% if preload_image %}

  {% assign aspect_ratio_desktop = image_desktop.aspect_ratio %}


  :root {
    --aspectRatioDesktop{{id}}: {{image_desktop.aspect_ratio}};
      --aspectRatioMobile{{id}}: {% if image_mobile == blank %}  {{image_desktop.aspect_ratio}} {% else %}  {{image_mobile.aspect_ratio}} {% endif %};
    }

      .single-image-{{id}} .single-image-container{
        position: relative;
        height: 0;
        width: 100%;
        padding-top: calc(100% / (var(--aspectRatioDesktop{{id}})));
          display: block;
          }

      .single-image-{{id}} .single-image-container .single-image-img{
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        object-fit: cover;
      }

      .single-image-{{id}} img{
        width: 100%;
      }

      @media (max-width: 767px){


        {% assign aspect-ratio = image_mobile.aspect_ratio %}


        .single-image-{{id}} .single-image-container{

          padding-top: calc(100% / (var(--aspectRatioMobile{{id}}))) !important;
            }

      }

      {% endif %}


</style>


{% endif %}


{% schema %}
{
"name": "Responsive Banner (PRO)",
"class": "ezfy-responsive-banner",
"settings": [
{
"type": "header",
"content": "Brought to you by ezfycode.com"
},
{
"type": "image_picker",
"id": "image-desktop",
"label": "Image for Desktop",
"info": "1200 x 600px recommended."
},
{
"type": "image_picker",
"id": "image-mobile",
"label": "Image for Mobile (optional)",
"info": "764 x 382px recommended. If left empty desktop image will be displayed instead."
},
{
"type": "url",
"id": "button_link",
"label": "Link"
},
{
"type": "checkbox",
"id": "open-new-tab",
"label": "Open on a new tab",
"default": true
},
{
"type": "range",
"id": "bottom-spacing",
"min": 0,
"max": 100,
"step": 1,
"unit": "px",
"label": "Bottom margin",
"info": "Default: 0",
"default": 0
},
{
"type": "range",
"id": "top-spacing",
"min": 0,
"max": 100,
"step": 1,
"unit": "px",
"label": "Top margin",
"info": "Default: 0",
"default": 0
},
{
"type": "text",
"id": "image-alt",
"label": "Image's alt text",
"info": "Briefly describe the image, helps with SEO and accessibility."
},
{
"type": "checkbox",
"id": "preload-image",
"label": "Preload image?",
"info": "Preloads the image while loading the page.",
"default": true
}
],
"presets": [
{
"name": "Responsive Banner (PRO)",
"category": "Custom by EZFY"
}
]
}
{% endschema %}


